<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="style/styleSearchShip.css">
    <title>Search Ship-Painel</title>
</head>
<body>
    <div id="container">
        <div id="content">
            <div class="loginHeader">
                <p>Search Ship</p>
            </div>
            <section id="main">
                <ul>
                    <% searchValue[0].forEach((searchValueElement,i)=>{ %>
                        <li>
                            <div class="vessel">
                                <%=searchValue[0][i]%>
                            </div>
                            <div class="situation">
                                <%=searchValue[1][i]%>
                            </div>
                        </li>
                    <% }) %>
                </ul>
            </section>
            <div class="divbutton">
                <a href="/CVS" onclick="loading()"><button id="buttonExport">Export CVS</button></a>
                <a href="/"><button id="buttonBack">Home</button></a>
            </div>
        </div>
    </div>
    <script>
        function loading(){
            alert("Realizando a tarefa... Quando pronto, o arquivo sera baixado automaticamente!")
        }
    </script>
</body>
</html>